  "scripts": {
    "start": "concurrently --kill-others \"npm run prod-web\"",
    "dev": "concurrently --kill-others \"npm run dev-web\"",
    "dev-ui": "webpack-dev-server --hot --progress --host 0.0.0.0 --config ./webpack.config.ui.js",
    "prod-web": "bnr prod-web",
    "dev-web": "bnr dev-web",
    "lint": "eslint src",
    "lint-fix": "eslint src --fix"
  },
  "betterScripts": {
    "prod-web": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "dev-web": {
      "command": "node ./bin/server.js"
    }
  },
