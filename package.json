{
  "name": "map-prototype",
  "version": "0.0.2",
  "description": "Prototype for chronist-project",
  "spdx": false,
  "license": "copyright",
  "scripts": {
    "start": "concurrently --kill-others \"npm run prod-web\" \"npm run prod-api\"",
    "dev": "concurrently --kill-others \"npm run dev-web\" \"npm run dev-api\"",
    "prod-web": "bnr prod-web",
    "prod-api": "bnr prod-api",
    "dev-web": "bnr dev-web",
    "dev-api": "bnr dev-api",
    "lint": "eslint src"
  },
  "betterScripts": {
    "prod-web": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "prod-api": {
      "command": "node ./bin/api.js",
      "env": {
        "NODE_PATH": "./api",
        "NODE_ENV": "production"
      }
    },
    "dev-web": {
      "command": "node ./bin/server.js"
    },
    "dev-api": {
      "command": "node ./bin/api.js",
      "env": {
        "NODE_PATH": "./api"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.morlov.tk/chronist/map-prototype.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack"
  ],
  "author": "Michael Orlov",
  "bugs": {
    "url": "https://gitlab.morlov.tk/chronist/map-prototype/issues"
  },
  "homepage": "https://gitlab.morlov.tk/chronist/map-prototype",
