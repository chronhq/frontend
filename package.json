{
  "name": "map-prototype",
  "version": "0.0.2",
  "description": "Prototype for chronist-project",
  "spdx": false,
  "license": "copyright",
  "scripts": {
    "start": "concurrently --kill-others \"npm run start-dev\" \"npm run start-dev-api\"",
    "dev": "concurrently --kill-others \"npm run start-dev-ui\" \"npm run start-dev-api\"",
    "start-dev": "bnr start-dev",
    "start-dev-ui": "bnr start-dev-ui",
    "start-dev-api": "bnr start-dev-api",
    "lint": "eslint src"
  },
  "betterScripts": {
    "start-dev-ui": {
      "command": "node ./bin/server.js",
      "env": {
        "FEATURES": [
          "UI",
          "DESU"
        ],
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000,
        "HOST": "localhost",
        "APIPORT": 3030
      }
    },
    "start-dev": {
      "command": "node ./bin/server.js",
      "env": {
        "FEATURES": [
          "MAINLINE"
        ],
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000,
        "HOST": "localhost",
        "APIPORT": 3030
      }
    },
    "start-dev-api": {
      "command": "node ./bin/api.js",
      "env": {
        "NODE_PATH": "./api",
        "NODE_ENV": "development",
        "HOST": "localhost",
        "APIPORT": 3030
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.morlov.tk/chronist/map-prototype.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack"
  ],
  "author": "Michael Orlov",
  "bugs": {
    "url": "https://gitlab.morlov.tk/chronist/map-prototype/issues"
  },
  "homepage": "https://gitlab.morlov.tk/chronist/map-prototype",
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-display-name": "^6.22.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "eslint": "^3.14.1",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "react-hot-loader": "^1.3.1",
    "redux-devtools": "^3.3.2",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.2.0",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.15.0"
  },
  "dependencies": {
    "better-npm-run": "0.0.14",
    "concurrently": "^3.1.0",
    "d3": "^4.5.0",
    "esri-to-geojson": "^1.0.2",
    "express": "^4.14.0",
    "http-proxy": "^1.16.2",
    "isomorphic-fetch": "^2.2.1",
    "object-sizeof": "^1.1.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0",
    "redux-promise-middleware": "^4.2.0",
    "redux-saga": "^0.14.3",
    "shapefile": "^0.6.2",
    "topojson-client": "^2.1.0",
    "topojson-server": "^2.0.0",
    "topojson-simplify": "^2.0.0"
  }
}
